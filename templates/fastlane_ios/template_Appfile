for_platform :ios do
    team_id ENV['TEAM_ID']
    for_lane :build_fad_ios_alpha do
        app_identifier '_PACKAGE_NAME_.alpha'
    end

    for_lane :build_fad_ios_beta do
        app_identifier '_PACKAGE_NAME_.beta'
    end

    for_lane :build_ios_production do
        app_identifier '_PACKAGE_NAME_'
    end
end